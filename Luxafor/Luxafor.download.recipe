<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads the latest version of Luxafor.</string>
        <key>Identifier</key>
        <string>com.github.smithjw.download.Luxafor</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Luxafor</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.2.0</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Processor</key>
                <string>URLTextSearcher</string>
                <key>Arguments</key>
                <dict>
                    <key>url</key>
                    <string>http://luxafor.com/download</string>
                    <key>request_headers</key>
                    <dict>
                        <key>User-Agent</key>
                        <string>Safari 8.0.2</string>
                    </dict>
                    <key>re_pattern</key>
                    <string>(luxafor_mac_v[0-9]+.[0-9]+.[0-9]+.zip)</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>URLDownloader</string>
                <key>Arguments</key>
                <dict>
                    <key>filename</key>
                    <string>%match%</string>
                    <key>url</key>
                    <string>http://luxafor.com/uploads/%match%</string>
                </dict>
            </dict>
        </array>
    </dict>
</plist>
